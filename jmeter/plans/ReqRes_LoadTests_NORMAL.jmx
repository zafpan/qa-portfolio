            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor (429 backoff)" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">/**
 * JSR223 PostProcessor (Groovy)
 * If a sampler returns HTTP 429 (Too Many Requests), pause the CURRENT JMeter thread
 * for Retry-After seconds (if provided) + a small jitter to avoid synchronized retries.
 */

import java.util.concurrent.ThreadLocalRandom // for jitter generation

// PostProcessor runs after each sampler in scope.
// `prev` = SampleResult of the sampler that just finished.
if (prev != null &amp;&amp; prev.getResponseCode() == &apos;429&apos;) {

    // Extract Retry-After header (seconds), if present
    String retryAfter = prev.getResponseHeaders() // retrieves all response headers
        ?.readLines() // split into a list of lines
        ?.find { it.toLowerCase().startsWith(&apos;retry-after:&apos;) }
        ?.split(&apos;:&apos;, 2)
        ?.last()
        ?.trim()

    int waitSeconds = 60 // fallback if header is missing/invalid
    if (retryAfter?.isInteger()) {
        waitSeconds = retryAfter.toInteger()
    }

    // Minimal jitter: 0â€“2000ms so threads don&apos;t all resume at the same instant
    long jitterMs = ThreadLocalRandom.current().nextLong(0L, 2001L)

    log.warn(&quot;429 hit. Backing off for ${waitSeconds}s + ${jitterMs}ms (Retry-After + jitter).&quot;)

    // Pause ONLY this thread before it proceeds to the next sampler/iteration
    Thread.sleep(waitSeconds * 1000L + jitterMs)
}</stringProp>
            </JSR223PostProcessor>
